{
	"manifest_version": 2,
	"name": "stocka",
	"description": "my chrome extension",
	"version": "1",
	"author": "aaron chapman",
	"browser_action": {"default_popup": "stocka.html"},
	"chrome_url_overrides" : {"newtab": "stocka.html"},
	"permissions": ["activeTab", "storage"],
	"content_scripts": [{"matches": ["<all_urls>"], "js": ["stocka.js", "jquery.js"]}]
}